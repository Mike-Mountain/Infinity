{
  "version": 1,
  "projects": {
    "blog": {
      "projectType": "application",
      "root": "apps/blog",
      "sourceRoot": "apps/blog/src",
      "prefix": "blog-app",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:webpack-browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/blog",
            "index": "apps/blog/src/index.html",
            "main": "apps/blog/src/main.ts",
            "polyfills": "apps/blog/src/polyfills.ts",
            "tsConfig": "apps/blog/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/blog/src/favicon.ico", "apps/blog/src/assets"],
            "styles": ["apps/blog/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/blog/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "config/environment.ts",
                  "with": "config/prod/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/blog/webpack.prod.config.js"
              }
            },
            "dev": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "config/environment.ts",
                  "with": "config/dev/environment.dev.ts"
                }
              ]
            },
            "local": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "config/environment.ts",
                  "with": "config/local/environment.local.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "blog:build:production"
            },
            "dev": {
              "browserTarget": "blog:build:dev"
            },
            "local": {
              "browserTarget": "blog:build:local"
            }
          },
          "defaultConfiguration": "local",
          "options": {
            "port": 4201
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "blog:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/blog/src/**/*.ts",
              "apps/blog/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/blog"],
          "options": {
            "jestConfig": "apps/blog/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "blog-e2e": {
      "root": "apps/e2e/blog-e2e",
      "sourceRoot": "apps/e2e/blog-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/e2e/blog-e2e/cypress.json",
            "devServerTarget": "blog:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blog:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/e2e/blog-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["blog"]
    },
    "blog-lib": {
      "projectType": "library",
