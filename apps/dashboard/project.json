{
  "projectType": "application",
  "root": "apps/dashboard",
  "sourceRoot": "apps/dashboard/src",
  "prefix": "dash",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/dashboard",
        "index": "apps/dashboard/src/index.html",
        "main": "apps/dashboard/src/main.ts",
        "polyfills": "apps/dashboard/src/polyfills.ts",
        "tsConfig": "apps/dashboard/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/dashboard/src/favicon.ico",
          "apps/dashboard/src/assets"
        ],
        "styles": ["apps/dashboard/src/styles.scss"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/dashboard/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "config/environment.ts",
              "with": "config/prod/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/dashboard/webpack.prod.config.js"
          }
        },
        "dev": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "config/environment.ts",
              "with": "config/dev/environment.dev.ts"
            }
          ]
        },
        "local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "config/environment.ts",
              "with": "config/local/environment.local.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "dashboard:build:production"
        },
        "dev": {
          "browserTarget": "dashboard:build:dev"
        },
        "local": {
          "browserTarget": "dashboard:build:local"
        }
      },
      "defaultConfiguration": "local",
      "options": {
        "port": 5000,
        "publicHost": "http://localhost:5000"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "dashboard:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/dashboard/src/**/*.ts",
          "apps/dashboard/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/dashboard"],
      "options": {
        "jestConfig": "apps/dashboard/jest.config.js",
        "passWithNoTests": true
      }
    },
    "serve-mfe": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx run blog:serve", "nx run dashboard:serve"]
      }
    },
    "serve-mfe-dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run blog:serve -c=dev",
          "nx run dashboard:serve -c=dev"
        ]
      }
    },
    "test-mfe": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run blog:test --codeCoverage",
          "nx run dashboard:test --codeCoverage",
          "nx run blog-lib:test --codeCoverage",
          "nx run dashboard-lib:test --codeCoverage",
          "nx run feature:test --codeCoverage",
          "nx run layout:test --codeCoverage",
          "nx run navigation:test --codeCoverage",
          "nx run schemas:test --codeCoverage"
        ]
      }
    },
    "build-mfe": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx run blog:build", "nx run dashboard:build"]
      }
    }
  },
  "tags": []
}
